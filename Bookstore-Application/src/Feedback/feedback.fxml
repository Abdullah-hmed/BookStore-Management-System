<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.geometry.Pos?>
<?import java.net.URL?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="Feedback.FeedbackController">
    <stylesheets>
        <URL value="@feedback.css" />
    </stylesheets>
    <children>
        <Label layoutX="14.0" layoutY="30.0" text="Name:" />
        <TextField fx:id="nameTextField" layoutX="100.0" layoutY="25.0" promptText="Your Name" />
        <Label layoutX="14.0" layoutY="80.0" text="Phone:" />
        <TextField fx:id="phoneTextField" layoutX="100.0" layoutY="75.0" promptText="Your Phone Number" />
        <Label layoutX="14.0" layoutY="130.0" text="Email:" />
        <TextField fx:id="emailTextField" layoutX="100.0" layoutY="125.0" promptText="Your Email" />
        <Label layoutX="14.0" layoutY="180.0" text="Feedback:" />
        <TextField fx:id="feedbackTextField" layoutX="100.0" layoutY="175.0" prefHeight="80.0" promptText="Your Feedback" />
        <Button layoutX="150.0" layoutY="270.0" onAction="#handleSubmitButtonAction" text="Submit" />
    </children>
</AnchorPane>









